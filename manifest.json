{
  "manifest_version": 3,
  "name": "YukaYukaGang",
  "description": "YukaYukaGambino",
  "version": "0.4",
  "author": "Valleteau Andrew",
  "homepage_url": "https://www.yuka.com",
  "content_scripts": [{
    "matches": ["https://app.pluralsight.com/*"],
    "js": ["jquery.min.js", "script.js"],
    "run_at": "document_end"
